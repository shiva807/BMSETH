        android:id="@+id/et8"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:background="@drawable/edittext_round"
        android:ems="10"
        android:hint="Branch"
        android:inputType="textPersonName"
        android:textColor="#061922"
