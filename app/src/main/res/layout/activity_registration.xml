<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/green_bg1"
    tools:context=".RegistrationActivity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:shadowColor="#68A0B0"
        android:shadowDx="8.0"
        android:shadowDy="8.0"
        android:shadowRadius="8"
        android:text="SIGN UP"
        android:textColor="#001101"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049" />


    <EditText
        android:id="@+id/et2"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:background="@drawable/greenish_gray"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:textColor="#013C03"
        android:textColorHint="#013C03"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184" />

    <EditText
        android:id="@+id/et4"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:background="@drawable/greenish_gray"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:textColor="#013C03"
        android:textColorHint="#013C03"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.284" />

    <EditText
        android:id="@+id/et5"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:background="@drawable/greenish_gray"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textColor="#013C03"
        android:textColorHint="#013C03"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.375" />

    <EditText
        android:id="@+id/et6"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:background="@drawable/greenish_gray"
        android:ems="10"
        android:hint="USN"
        android:inputType="textPersonName"
        android:textColor="#013C03"
        android:textColorHint="#013C03"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.459" />

